{
    "latex-workshop.latex.outDir": "%DIR%/bin",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.recipes": [
        {
          "name": "latexmk ➞ copyPDF",
          "tools": ["latexmk", "copyPDF"]
        }
      ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "copyPDF",
            "command": "cmd.exe",
            "args": [
                "/c", 
                "copy",  
                "%OUTDIR_W32%\\%DOCFILE%.pdf",
                "%DIR_W32%",
            ],
            "env": {}
        },
    ],
    "cSpell.language": "en,de",
